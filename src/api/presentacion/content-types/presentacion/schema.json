{
  "kind": "collectionType",
  "collectionName": "presentacions",
  "info": {
    "singularName": "presentacion",
    "pluralName": "presentacions",
    "displayName": "Presentacion",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Cargo": {
      "type": "string",
      "required": false
    },
    "Celular": {
      "type": "biginteger"
    },
    "Correo": {
      "type": "email",
      "required": true
    },
    "servicios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::servicio.servicio"
    },
    "id_own_user": {
      "type": "biginteger",
      "required": false
    },
    "Nombre_Responsable": {
      "type": "string",
      "required": true
    },
    "DownloadPDF": {
      "type": "string"
    },
    "Estado": {
      "type": "enumeration",
      "enum": [
        "Borrador",
        "Enviado"
      ],
      "default": "Borrador"
    },
    "empresa": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::empresa.empresa",
      "inversedBy": "presentacions"
    },
    "valor_agregado": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::valor-agregado.valor-agregado",
      "inversedBy": "presentacions"
    },
    "ValorAgregadoPDF": {
      "type": "string"
    },
    "Nombre_Empresa": {
      "type": "string",
      "required": true
    }
